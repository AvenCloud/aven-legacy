{
  "name": "aven",
  "version": "0.0.0",
  "scripts": {
    "start": "NODE_ENV=development forever -c 'node -r babel-register' ./forever-config.json",
    "run-prod": "NODE_ENV=production node lib/AppServer.js",
    "run-test-env": "NODE_ENV=test node lib/AppServer.js",
    "build-web-main": "node_modules/babel-cli/bin/babel.js src/ --out-dir lib/",
    "build-web-common": "rm lib/common.js && mkdir -p lib/common && cp -rf mobile/common .build-common && node_modules/babel-cli/bin/babel.js .build-common/ --out-dir lib/common/ && rm -rf .build-common",
    "build-web-static": "rm -rf lib/static && cp -r src/static lib/static",
    "build-web": "npm run build-web-main && npm run build-web-common && npm run build-web-static",
    "build-client": "node_modules/browserify/bin/cmd.js lib/ProjectIndex.js -o lib/static/ProjectIndex.js",
    "build": "npm run build-web && npm run build-client",
    "upload-secrets": "node lib/upload_secrets.js",
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "jest": {
    "transform": {},
    "rootDir": "src"
  },
  "bin": {
    "aven": "./lib/CLI.js"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^1.9.2",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "browserify": "^14.4.0",
    "bufferutil": "3.0.1",
    "commander": "^2.11.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "crypto-js": "^3.1.9-1",
    "denodeify": "^1.2.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "file-type": "^7.2.0",
    "forever": "^0.15.3",
    "js-cookie": "^2.2.0",
    "moment": "^2.18.1",
    "multer": "^1.3.0",
    "node-fetch": "^1.7.0",
    "pg": "^6.2.3",
    "prompt": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "redis": "^2.8.0",
    "utf-8-validate": "3.0.2",
    "validator": "^7.0.0",
    "watchr": "^3.0.1",
    "ws": "^3.2.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "jest": "^20.0.4",
    "regenerator-runtime": "^0.10.5"
  }
}
